{
    "name": "phev-view",
    "version": "1.0.0",
    "description": "Phev code viewer",
    "main": "index.js",
    "scripts": {
        "test": "npm run test-unit",
        "start": "node index.js",
        "start-dev": "cross-env NODE_ENV=\"production\" webpack-dev-server --hot",
        "test-unit": "cross-env NODE_ENV=test DEBUG=true mocha --reporter mocha-circleci-reporter --require babel-polyfill --compilers js:babel-core/register --recursive ./src/**/*.spec.js",
        "test-watch": "cross-env NODE_ENV=test DEBUG=true mocha --watch --require babel-polyfill --compilers js:babel-core/register --recursive ./src/**/*.spec.js",
        "clean": "rimraf lib",
        "build": "npm run clean && babel src -d lib && webpack -p",
        "deploy": "cross-env NODE_ENV=\"production\" webpack -p && firebase deploy"
    },
    "author": "Jamie Nuttall",
    "license": "ISC",
    "dependencies": {
        "firebase": "^4.5.0",
        "phev-utils": "github:papawattu/phev-utils",
        "react-router-dom": "^4.2.2"
    },
    "devDependencies": {
        "babel-cli": "^6.26.0",
        "babel-core": "^6.26.0",
        "babel-loader": "^7.1.2",
        "babel-plugin-transform-async-to-generator": "^6.24.1",
        "babel-plugin-transform-react-jsx": "^6.24.1",
        "babel-preset-env": "^1.6.0",
        "babel-preset-react": "^6.24.1",
        "chai": "^3.5.0",
        "cross-env": "^4.0.0",
        "extract-text-webpack-plugin": "^3.0.1",
        "file-loader": "^0.11.2",
        "immutable": "^3.8.2",
        "jquery": "^3.2.1",
        "jsdom": "^11.3.0",
        "mocha": "^3.5.3",
        "mocha-circleci-reporter": "0.0.2",
        "mocha-junit-reporter": "^1.15.0",
        "pretty": "^2.0.0",
        "react": "^15.6.2",
        "react-dom": "^15.6.2",
        "resolve-url-loader": "^2.1.0",
        "rimraf": "^2.6.2",
        "rxjs": "^5.4.2",
        "sinon": "^2.3.5",
        "url-loader": "^0.5.9",
        "webpack": "^3.6.0",
        "webpack-dev-middleware": "^1.11.0",
        "webpack-dev-server": "^2.9.1",
        "webpack-hot-middleware": "^2.19.0"
    }
}
